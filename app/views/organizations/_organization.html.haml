%tr
  %td
    = link_to organization.name, organization_path(organization)
  %td
    = organization.duns
  %td
    = link_to 'Edit', edit_organization_path(organization)
    |
    = link_to 'Delete', organization_path(organization), :data => { :confirm => 'Are you sure you want to delete this organization?' }, :method => :delete
