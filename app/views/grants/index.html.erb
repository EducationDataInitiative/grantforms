<h1>All Grants</h1>

<h2>Search</h2>
<%= search_form_for @q do |f| %>
  <%= f.label :name_cont %>
  <%= f.text_field :name_cont %><br/>
  <%= f.label :description_cont %>
  <%= f.text_field :description_cont %>
  <%= f.submit %>
<% end %>


<%= will_paginate %>

<ul>
  <%= render @grants %>
</ul>

<%= will_paginate %>


